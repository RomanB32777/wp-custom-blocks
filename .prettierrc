{
	"trailingComma": "es5",
	"printWidth": 80,
	"semi": true,
	"useTabs": true,
	"tabWidth": 2,
	"singleQuote": false,
	"plugins": [
		"@wordpress/prettier-config",
		"@ianvs/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"<BUILTIN_MODULES>",

		"",

		"^react$",
		"<THIRD_PARTY_MODULES>",
		"^@wordpress/(.*)$",

		"",

		"^@/(.*)$",

		"",

		"^../(.*)$",
		"^./(.*)[^(.scss)]$",
		"^[.]",

		"",

		"(.scss)$"
	]
}
